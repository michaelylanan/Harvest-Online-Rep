<!--#endregion -->
<div>
    <!--#region Table of options -->
    @*<form style="margin-bottom:10px">
            <input type="text" placeholder="Search.." name="search">
            <button type="submit"><i class="fa fa-search"></i></button>
            <img src="~/lib/images/output-onlinejpgtools (3).png" height="30" width="30" />
        </form>
        <table width="100%" style="border-top:2px solid black; border-bottom:2px solid black; color:black; font-size:20px">
            <tr>
                <th width="22%"><a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a></th>
                <th width="22%" ><a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="About">About Us</a></th>
                <th width="22%">Products</th>
                <th width="23%">Farmers</th>
                <th>Contact Us</th>
            </tr>
        </table>*@
    <!--#endregion -->
    <img src="~/lib/images/banner.jpg" style="width:100%; position:relative" />
    <h2 style="text-align:center; margin-top:30px; color:black"> <b>Collections</b> </h2>

    <div id="myCarousel" class="carousel slide" data-ride="carousel" style="background-color:green; margin-bottom:100px">
        <!-- Indicators -->
        <ol class="carousel-indicators">
            <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
            <li data-target="#myCarousel" data-slide-to="1"></li>
            <li data-target="#myCarousel" data-slide-to="2"></li>
        </ol>

        <!-- Wrapper for slides -->
        <div class="carousel-inner">
            <div class="item active">
                <img src="/lib/images/veg.jpg" alt="Vegetables" width="460" height="345" style="display:block; margin-left:auto;margin-right:auto">
                <div class="carousel-caption">
                    <h3>Vegetables</h3>
                </div>
            </div>

            <div class="item">
                <img src="/lib/images/fruits.jpg" alt="Fruits" width="460" height="345" style="display:block; margin-left:auto;margin-right:auto">
                <div class="carousel-caption">
                    <h3>Fruits</h3>
                </div>
            </div>

            <div class="item">
                <img src="~/lib/images/promo.png" alt="Promo" width="460" height="345" style="display:block; margin-left:auto;margin-right:auto">
                <div class="carousel-caption">
                    <h3>Promo</h3>
                </div>
            </div>
        </div>

        <!-- Left and right controls -->
        <a class="left carousel-control" href="#myCarousel" data-slide="prev">
            <span class="glyphicon glyphicon-chevron-left"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="right carousel-control" href="#myCarousel" data-slide="next">
            <span class="glyphicon glyphicon-chevron-right"></span>
            <span class="sr-only">Next</span>
        </a>        
    </div>
</div>

